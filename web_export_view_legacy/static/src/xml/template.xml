            expr="//t[@t-if='nbTotal and !nbSelected and activeActions.exportXlsx and isExportEnable and !env.isSmall']"
            position="attributes"
        >
            <attribute name="t-if">true</attribute>
        </xpath>
    </t>
</templates>
